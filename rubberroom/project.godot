; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="RubberRoom"
run/main_scene="res://scenes/Levels/world.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
RoomManager="*res://scripts/Managers/RoomManager.gd"
SceneManager="*res://scripts/Managers/SceneManager.gd"
EnemySpawnManager="*res://scripts/Managers/EnemySpawnManager.gd"
PlayerManager="*res://scripts/Managers/PlayerManager.gd"

[dialogue_manager]

general/balloon_path="res://assets/Dialouge/balloon.tscn"

[display]

window/stretch/mode="canvas_items"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[dotnet]

project/assembly_name="RubberRoom"

[editor_plugins]

enabled=PackedStringArray("res://addons/beehave/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "yellow",
"res://assets/": "red",
"res://data/": "teal",
"res://scenes/": "blue",
"res://scripts/": "green"
}

[global_group]

enemies=""
player=""
interactable=""
spawnpoint=""
EnemySpawnMarker="Places where enemies will spawn"
"nav region"=""
cover=""
projectile=""
weapon=""

